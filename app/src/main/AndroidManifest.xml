<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ieee">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CollegeEventActivity"
            android:label="College Events"
            android:parentActivityName=".AfterCommitteeLoginActivity"
            android:theme="@style/NoAppTheme"/>
        <activity
            android:name=".ProfileActivity"
            android:label="ETR Person&apos;s Profile"
            android:parentActivityName=".AfterCommitteeLoginActivity"
            android:theme="@style/NoAppTheme" />
        <activity android:name=".AfterMemberLoginActivity" />
        <activity android:name=".CommitteeRegisterActivity" />
        <activity android:name=".AfterCommitteeLoginActivity" />
        <activity android:name=".MemberRegistrationActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>